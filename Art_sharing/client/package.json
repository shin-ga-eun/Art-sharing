{
    "name": "art-sharing-client",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@material-ui/core": "^4.1.3",
        "@material-ui/icons": "^4.2.1",
        "axios": "^0.19.0",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-live-clock": "^3.1.0",
        "react-router-dom": "^5.0.1",
        "react-scripts": "3.0.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "parser": "babel-eslint",
        "extends": "airbnb",
        "plugins": [
            "react",
            "jsx-a11y",
            "import"
        ],
        "parserOptions": {
            "ecmaVersion": 2016,
            "sourceType": "module"
        },
        "env": {
            "es6": true,
            "browser": true,
            "node": true
        },
        "rules": {
            "linebreak-style": 0,
            "quotes": [
                "error",
                "double",
                {
                    "avoidEscape": true
                }
            ],
            "indent": [
                "error",
                4
            ],
            "react/jsx-indent": [
                "error",
                4
            ],
            "react/jsx-indent-props": [
                "error",
                4
            ],
            "react/prefer-stateless-function": 0,
            "react/jsx-one-expression-per-line": 0,
            "react/jsx-filename-extension": [
                1,
                {
                    "extensions": [
                        ".js",
                        ".jsx"
                    ]
                }
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "proxy": "http://192.168.35.88:8080/",
    "devDependencies": {
        "eslint-config-airbnb": "^17.1.1"
    }
}
